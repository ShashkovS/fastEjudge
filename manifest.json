{
  "name": "Модификатор ejudge",
  "description": "Улучшение ejudge",
  "version": "1.0.5",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://server.179.ru/cgi-bin/new-master?*", "https://server.179.ru/cgi-bin/new-judge?*"],
      "js": ["highlight.min.js", "content-script.js"],
      "css": ["highlight.pack.idea.css"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}
