{
  "name": "Модификатор ejudge",
  "description": "Улучшение ejudge",
  "version": "1.0.8",
  "manifest_version": 3,
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://*/cgi-bin/new-master?*", "https://*/cgi-bin/new-judge?*"],
      "js": ["highlight.min.js", "content-script.js"],
      "css": ["highlight.pack.idea.css"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "/images/16.png",
      "32": "/images/32.png",
      "48": "/images/48.png",
      "128": "/images/128.png"
    }
  },
  "icons": {
    "16": "/images/16.png",
    "32": "/images/32.png",
    "48": "/images/48.png",
    "128": "/images/128.png"
  }
}
